generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Travel {
  id Int @id @default(autoincrement())
  slug String? @unique
  title String?
  image String ?
  summary String?
  instructions String?
  creator String?
  creator_email String?
}
